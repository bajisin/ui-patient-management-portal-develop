  "scripts": {
    "dev": "npm run husky:prepare && webpack serve --config ./config/webpack/dev.js --mode development --color",
    "lint": "eslint --fix .",
    "format": "prettier --write \"./**/*.{js,jsx,css,scss,json}\" --config .prettierrc",
    "build": "npm run build:develop",
    "build:develop": "webpack --config ./config/webpack/dev.js --mode development && npm run build:server",
    "build:staging": "webpack --config ./config/webpack/stage.js --mode development && npm run build:server",
    "build:preprod": "webpack --config ./config/webpack/preprod.js --mode development && npm run build:server",
    "build:prod": "webpack --config ./config/webpack/prod.js --mode development && npm run build:server",
    "build:production": "webpack --config ./config/webpack/build.js --mode production && npm run build:server",
    "build:server": "webpack --config ./config/webpack/server.js --mode production",
    "husky:prepare": "npx husky install",
    "test": "cypress run --headless",
    "test:coverage": "cypress run --headless && nyc report --reporter=lcov",
    "coverage": "npx nyc report --reporter=text-summary",
    "cy:open": "cypress open",
    "localApi": "json-server --watch ./fakeJsonApi/db.json --routes ./fakeJsonApi/routes.json --port 3030",
    "cypress:run-all": "npx cypress run --spec '**/*'",
    "sonar": "sonarscan.js",
    "sonarfinal": "node ./sonarqube/sonarscan.js"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.5.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
